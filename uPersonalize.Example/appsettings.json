{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "server=.;database=uPersonalize;user id=sa;password='12345'"
  },
  "Umbraco": {
    "CMS": {
        "WebRouting": {
            "UmbracoApplicationUrl": "https://localhost:44386"
        },
        "Hosting": {
            "Debug": true
        },
        "RuntimeMinification": {
            "useInMemoryCache": true,
            "cacheBuster": "Timestamp"
        },
        "Content": {
            "MacroErrors": "Throw",
            "ContentVersionCleanupPolicy": {
                "EnableCleanup": true
            }
        },
        "Global": {
            "UseHttps": true,
            "Id": "a2de54a7-44c5-4719-be1a-6bf9a1b1b104",
            "Smtp": {
                "From": "your@email.here",
                "Host": "localhost",
                "Port": 25
            }
        }
    }
  }
}